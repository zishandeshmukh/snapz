
{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Backend Server",
            "program": "${workspaceFolder}/Backend/Server/gemini_server.js",
            "cwd": "${workspaceFolder}/Backend/Server"
        },
        {
            "type": "chrome",
            "request": "launch",
            "name": "Launch Frontend",
            "url": "http://localhost:8080",
            "webRoot": "${workspaceFolder}/Frontend"
        },
        {
            "name": "Debug Android",
            "request": "launch",
            "type": "react-native",
            "cwd": "${workspaceFolder}/mobilef",
            "platform": "android"
        }
    ]
}
